#include<stdio.h>
int main()
{
    int a,b;
    printf("enter the value of a= ");
    scanf("%d",&a);
    printf("value of a=%d \n",a);
    printf("enter the value of b=");
    scanf("%d",&b);
    printf("value of b=%d \n",b);
    if(a>b)
    printf("a is the largest number");
    else
    printf("b is the largest number");
    return 0;
    
}
